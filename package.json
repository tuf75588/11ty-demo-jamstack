{
  "name": "11ty-clone",
  "version": "1.0.0",
  "description": "",
  "main": ".eleventy.js",
  "scripts": {
    "build": "npm-run-all build:html build:css",
    "build:html": "eleventy",
    "build:css": "postcss src/css/tailwind.css -o dist/css/styles.css",
    "watch:html": "ELEVENTY_ENV=dev eleventy --watch --quiet",
    "watch:css": "postcss src/css/tailwind.css -o dist/css/styles.css --watch",
    "serve": "npx live-server dist --quiet --port=8090",
    "start": "npm-run-all --parallel watch:html watch:css serve",
    "clean": "rm -rf dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-cache-assets": "^2.3.0",
    "@zachleat/filter-container": "^2.0.3",
    "autoprefixer": "^10.4.0",
    "cssnano": "^5.0.10",
    "d3": "^7.1.1",
    "dotenv": "^10.0.0",
    "fast-glob": "^3.2.7",
    "gray-matter": "^4.0.3",
    "i18n-iso-countries": "^7.0.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "luxon": "^2.1.1",
    "markdown-it": "^12.2.0",
    "netlify-plugin-minify-html": "^0.3.1",
    "node-fetch": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "placename": "^1.1.2",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.2",
    "postcss-import": "^14.0.2",
    "spdx-correct": "^3.1.1",
    "tailwindcss": "^2.2.19"
  },
  "devDependencies": {
    "live-server": "^1.2.1"
  }
}
